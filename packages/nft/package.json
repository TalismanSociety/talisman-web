{
  "name": "@talismn/nft",
  "type": "module",
  "exports": "./build/src/index.js",
  "scripts": {
    "codegen:rmrk2": "yarn graphql-codegen --config graphql.config.cjs --project rmrk2",
    "codegen:statemine": "yarn graphql-codegen --config graphql.config.cjs --project statemine",
    "codegen:substrate-nft-kusama-asset-hub": "yarn graphql-codegen --config graphql.config.cjs --project substrate-nft-kusama-asset-hub",
    "codegen:substrate-nft-polkadot-asset-hub": "yarn graphql-codegen --config graphql.config.cjs --project substrate-nft-polkadot-asset-hub",
    "codegen:unique": "yarn graphql-codegen --config graphql.config.cjs --project unique",
    "codegen": "yarn codegen:rmrk2 && yarn codegen:statemine && yarn codegen:substrate-nft-kusama-asset-hub && yarn codegen:substrate-nft-polkadot-asset-hub && yarn codegen:unique",
    "dev": "tsc --build --watch",
    "build": "rm -rf build && yarn tsc --build",
    "check-types": "yarn tsc --build"
  },
  "dependencies": {
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "multiformats": "^11.0.2"
  },
  "devDependencies": {
    "@acala-network/types": "^5.1.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@polkadot/api": "^10.11.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "viem": "^1.20.3"
  },
  "peerDependencies": {
    "@polkadot/api": ">= 9.x",
    "viem": "1.x"
  }
}
