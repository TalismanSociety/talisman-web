{
  "private": true,
  "name": "@talismn/ui",
  "packageManager": "yarn@3.4.1",
  "main": "build/commonjs/index.js",
  "exports": {
    ".": {
      "import": "./build/module/index.js",
      "require": "./build/commonjs/index.js"
    },
    "./assets/css/talismn.css": "./assets/css/talismn.css"
  },
  "scripts": {
    "build": "rm -rf build && tsc -p tsconfig.build.json -m commonjs --outDir build/commonjs && tsc -p tsconfig.build.json -m esnext --outDir build/module",
    "check-types": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@floating-ui/react": "^0.19.2",
    "@lottiefiles/react-lottie-player": "^3.5.2",
    "@talismn/icons": "workspace:^"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@tsconfig/create-react-app": "^1.0.3",
    "@types/react": "^18.0.28",
    "babel-loader": "^8.3.0",
    "framer-motion": "^9.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@emotion/react": "11.x",
    "framer-motion": "9.x",
    "react": "18.x",
    "react-hot-toast": "2.x"
  }
}
