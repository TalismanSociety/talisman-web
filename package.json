    "@polkadot/api": "^5.4.1",
    "@polkadot/extension-dapp": "^0.38.4",
    "@polkadot/keyring": "^7.0.1",
    "@polkadot/react-identicon": "^0.80.1",
    "@talismn/api-react-hooks": "~0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.172",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.10",
    "babel-plugin-styled-components": "^1.13.2",
    "bignumber.js": "^9.0.1",
    "craco-alias": "^3.0.1",
    "graphql": "^15.5.1",
    "husky": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "react": "^17.0.2",
    "react-apollo-network-status": "^5.0.1",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "start": "CI= craco start",
    "build": "CI= GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-cycle": "warn"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,html,css,scss}": "prettier --write"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
